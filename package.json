{
  "name": "Raptors",
  "main": "server.js",
  "repository": "git@github.com:Libbay/Raptors.git",
  "scripts": {
    "start": "better-npm-run start-dev",
    "start:api": "concurrently --kill-others \"better-npm-run start-dev\" \"better-npm-run start-dev-api\"",
    "build:test": "better-npm-run build:test",
    "build:prod": "better-npm-run build:prod"
  },
  "betterScripts": {
    "start-dev": {
      "command": "babel-node ./server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "start-dev-api": {
      "command": "babel-node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "APIPORT": 3030
      }
    },
    "build:test": {
      "command": "rm -rf dist && webpack --verbose --colors --display-error-details --config webpack/webpack.test.babel.js",
      "env": {
        "NODE_PATH": "./dist",
        "NODE_ENV": "test"
      }
    },
    "build:prod": {
      "command": "rm -rf dist && webpack --verbose --colors --display-error-details --config webpack/webpack.prod.babel.js",
      "env": {
        "NODE_PATH": "./dist",
        "NODE_ENV": "production"
      }
    }
  },
  "author": "Libbay",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.5.2",
    "babel-polyfill": "^6.3.14",
    "history": "^1.13.1",
    "js-cookie": "^2.0.4",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.1",
    "react-router": "^1.0.2",
    "redux": "^3.0.0",
    "reqwest": "^2.0.5",
    "superagent": "^2.1.0",
    "superagent-jsonp": "^0.1.1"
  },
  "version": "0.0.1",
  "description": "A Common JavaScript Solution",
  "devDependencies": {
    "autoprefixer": "^6.1.2",
    "autoprefixer-loader": "~2.0.0",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^3.1.23",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "better-npm-run": "0.0.10",
    "body-parser": "^1.14.2",
    "bundle-loader": "^0.5.4",
    "colors": "^1.1.2",
    "compression-webpack-plugin": "^0.2.0",
    "concurrently": "^2.2.0",
    "css-loader": "^0.14.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-plugin-react": "^3.15.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.7.2",
    "img-loader": "^1.2.2",
    "ip": "^1.0.2",
    "json-loader": "^0.5.1",
    "jsx-loader": "^0.13.2",
    "less": "~2.5.1",
    "less-loader": "^2.2.0",
    "node-libs-browser": "^0.5.2",
    "open": "0.0.5",
    "postcss-loader": "^0.8.0",
    "precss": "^1.3.0",
    "react-hot-loader": "^1.2.7",
    "redux-logger": "^2.1.4",
    "redux-simple-router": "^0.0.10",
    "redux-thunk": "^1.0.0",
    "stats-webpack-plugin": "^0.2.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.9.10",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.9.0",
    "webpack-hot-middleware": "^2.2.0"
  }
}
